<h1>Users</h1>
<table>
    <tr>
        <th> User </th>
        <th> ID </th>
        <th> created_at </th>
        <th> updated_at </th>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <td> <%= user.name %> </td>
        <td> <%= user.id %> </td>
        <td> <%= user.created_at %> </td>
        <td> <%= user.updated_at %> </td>
    </tr>
    <% end %>
<a href="/users/new">Add new user! </a>
