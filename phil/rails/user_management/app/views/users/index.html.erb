<h2>All Users</h2>
<% if flash.notice %>
    <p><%= flash.notice %></p>
<% end %>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th></th>
    </tr>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.first_name %></td>
            <td><%= user.last_name %></td>
            <td><%= user.email %></td>
            <td>
                <a href="/users/<%= user.id %>">Show</a> |
                <a href="/users/<%= user.id %>/edit">Edit</a> |
                <%= link_to 'Delete', users_destroy_path(user.id), method: 'delete' %>
            </td>
        </tr>
    <% end %>
</table>
<p><a href="/users/new">New User</a></p>
