<div id="container">
	<h1>Listing Users </h1>
	<table>
	<tr>
		<th> First Name </th>
		<th> Last Name </th>
		<th> Email </th>
		<th> Action </th>
	</tr>
	<% @users.each do |user| %> 
	<tr>
	<td> 
	<%=user.first_name %>
	</td>
	<td>
	<%=user.last_name %>
	</td>
	<td>
	<%=user.email_address %>
	</td>
	<td> <a href="/users/<%= user.id %>"> Show </a> </td>
	<td> <a href="/users/<%= user.id %>/edit"> Edit </a> </td>
	<td> <a href="/users/<%= user.id %>/destroy" method="Post"> Delete </a> </td>
	</tr>
	<% end %>
	</table> 
	<a href="/users/new" method="Post"> New user </a>
</div>

